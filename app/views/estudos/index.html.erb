<%= admin_link 'Adcionar Novo Estudo','/estudos', class: 'btn btn-primary btn-lg', 'data-toggle' => 'modal' %>
    <% @estudos.each do |estudo| %>
     <div class="map-buttom span3" >
        <div class="options">
          <%= admin_link image_tag('pencil.png'), edit_estudo_path(estudo), title: 'editar' %>
          <%= link_to image_tag('eye.png'), estudo_path(estudo), title: 'visualizar', class: 'map-view img-thumbnail', height: '400px' %>
        </div>
        <img src="<%= estudo.image.thumb %>">
        <spam><%= estudo.title %></spam>

      </div>
    <% end %>
