<div class="span12">
	<div class="span4">
		<div class="map-buttom" >
		<div class="options">
		  <%= admin_link image_tag('pencil.png'), edit_map_path(map), title: 'editar' %>
		  <%= link_to image_tag('eye.png'), map_path(map), title: 'visualizar', class: 'map-view img-rounded' %>
		</div>
		<img src="<%= map.image.thumb %>">
		</div>
	</div>
	<div class="pull-right span7" >
		<h2><%= map.name %></h2>
		<p><%= map.info %></p>
	</div>
</div>